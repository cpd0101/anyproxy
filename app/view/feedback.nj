<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="keywords" content="{{ name }},Proxy,Google,Shadowsocks" />
    <meta name="description" content="Free Online Web Proxy 免费在线网页代理" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no" />
    <link rel="shortcut icon" href="/favicon.ico" /> 
    <title>{{ name }} 意见反馈</title>
    <link rel="stylesheet" href="https://gw.alipayobjects.com/os/rmsportal/cupayaBkBRxHECKcstzD.css" />
  </head>
  <body>
    <div style="position:relative;min-height:100%;overflow-x:hidden;">
      <div class="text-center" style="padding:30px 0 45px;">
        <a target="_blank" href="{{ href }}">
          <img width="300" height="80" src="{{ logo }}" />
        </a>
      </div>
      <div class="row" style="padding:0 0 90px;">
        <div class="col-sm-10 col-sm-offset-1 col-xs-12">
          <div id="disqus_thread"></div>
        </div>
      </div>
      <div class="text-center" style="position:absolute;bottom:0;width:100%;padding:15px 0;">
        &copy;{{ year }}&nbsp;
        <a target="_blank" href="/">{{ name }}</a>
      </div>
    </div>
    <script src="https://gw.alipayobjects.com/os/rmsportal/JdEpaOqbNgKDgeKLvRXV.js"></script>
    <script src="https://hm.baidu.com/hm.js?9ec911f310714b9fcfafe801ba8ae42a"></script>
    <script>
    (function() {
      var d = document, s = d.createElement('script');
      s.src = 'https://anyproxy.disqus.com/embed.js';
      s.setAttribute('data-timestamp', +new Date());
      (d.head || d.body).appendChild(s);
    })();
    </script>
    <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
    <script>
      $('#disqus_thread').delegate('a', 'mousedown', function (e) {
        if ($(this).text() === 'Reload') {
          var iframe = $('#disqus_thread').children('iframe');
          var href = iframe.get(0).src;
          var index = href.lastIndexOf('#');
          if (index === -1) {
            index = href.length;
          }
          var src = href.slice(0, index);
          var hash = href.slice(index);
          window.open('/proxy?target=' + btoa(encodeURI(src)) + hash);
        }
      });
      if (navigator.serviceWorker && location.protocol === 'https:') {
        (function () {
          var intervalId = null;
          intervalId && clearInterval(intervalId);
          intervalId = setInterval(function () {
            var iframe = $('#disqus_thread').children('iframe');
            if (iframe.length) {
              intervalId && clearInterval(intervalId);
              var href = iframe.get(0).src;
              var index = href.lastIndexOf('#');
              if (index === -1) {
                index = href.length;
              }
              var src = href.slice(0, index);
              var hash = href.slice(index);
              iframe.on('load', function (e) {
                intervalId && clearInterval(intervalId);
                intervalId = setInterval(function () {
                  var height = $(iframe.get(0).contentWindow.document).height();
                  if (height > 60) {
                    iframe.height(height);
                    $('#disqus_thread').children('div').remove();
                  }
                }, 1000 / 60);
              });
              iframe.attr('src', '/proxy?target=' + btoa(encodeURI(src)) + '&noframe=true' + hash);
            }
          }, 1000 / 60);
        })()
      }
    </script>
  </body>
</html>